<UserControl x:Class="RqCalc.Wpf.Controls.BuffCollectionControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:controls="clr-namespace:RqCalc.Wpf.Controls"
             mc:Ignorable="d"
             d:DesignHeight="300"
             d:DesignWidth="330">

    <ItemsControl ItemsSource="{Binding DataContext, RelativeSource={RelativeSource Self}}">
        <ItemsControl.ItemTemplate>
            <DataTemplate>
                <controls:StackObjectControl Margin="5" />
            </DataTemplate>
        </ItemsControl.ItemTemplate>
    </ItemsControl>
    
</UserControl>
